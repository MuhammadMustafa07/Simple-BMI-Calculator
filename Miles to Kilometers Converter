# Enter the amount you want to coonvert

Miles = 61

def KMtoMiles(Miles):
    print("1 Mile is equal to 1.6 Kilometers")
    Km = 1.609 * Miles
    txt = "{} Miles is equal to "
    print(txt.format(Miles))
    txt2 = "{} Kilometers"
    print(txt2.format(Km))
    return "Have a good day"
    
result = KMtoMiles(Miles)
print(result)
